{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["vue", "prettier"],
  "rules": {
    "no-undef": "warn",
    "vue/no-multiple-template-root": 0,
    "no-unused-vars": "warn"
  }
}
